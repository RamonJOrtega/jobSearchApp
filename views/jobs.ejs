<body>

    <h1> <%- title %> </h1>
    <h2> Open Positions: <%- jobCount %> </h2>
    <h2> Companies Hiring: <%- companyCount %> </h2>
    <h2> Openings per Company Sorted Most to Least: <br> </h2>
    <table>
        <tr>
            <th>NO.</th>
            <th>COMPANY</th>
            <th>OPENINGS</th>
        </tr>
        <% for( let index = 0; index < companyFreq.length; index++ ) { %>
            <tr>
                <td> <%- index +1 %> </td>
                <td> <a href="<%- index + 1 %> "> <%- companyFreq[index][0] %> </a> </td> 
                <td> <%- companyFreq[index][1] %> </td> 
         <% } %> 
            </tr>
    </table>

</body>