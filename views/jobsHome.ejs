<body>
    <section>
        <h1> <%- title %> </h1>
        <h2> Open Positions: <span> <%- jobCount %></span> </h2>
        <h2> Companies Hiring: <span><%- companyCount %> </span><br></h2>
        <table>
            <caption>            Table of Openings per Company Sorted Most to Least:
            </caption>
            <tr>
                <th>COMPANY NUMBER</th>
                <th>COMPANY NAME | LINK</th>
                <th>AVAILABLE JOBS</th>
            </tr>
            <% for( let index = 0; index < companyFreq.length; index++ ) { %>
                <tr>
                    <td> <%- index +1 %> </td>
                    <td> <a class="company" href="/company/<%- companyFreq[index][0] %>"> <%- companyFreq[index][0] %> </a> </td> 
                    <td> <%- companyFreq[index][1] %> </td>
            <% } %> 
                </tr>
        </table>
    </section>
</body>